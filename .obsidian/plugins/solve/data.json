{
  "engine": {
    "explicitMode": true
  },
  "interface": {
    "renderResultEndOfLine": false,
    "showStatusBarCompanion": false,
    "animateResults": true,
    "animationClass": "animate__fadeInLeft",
    "animationDuration": "200ms"
  },
  "inlineSolve": {
    "includeExpressionOnCommit": false,
    "includeBackticksOnCommit": true,
    "includeEqualsOnCommit": true
  },
  "variable": {
    "renderResult": true
  },
  "arithmeticProvider": {
    "enabled": true,
    "renderEqualsBeforeResult": true
  },
  "functionArithmeticProvider": {
    "enabled": true
  },
  "vectorArithmeticProvider": {
    "enabled": true
  },
  "percentageArithmeticProvider": {
    "enabled": true
  },
  "datetimeProvider": {
    "enabled": true,
    "parsingFormat": 0
  },
  "unitOfMeasurementProvider": {
    "enabled": true
  },
  "diceProvider": {
    "enabled": true
  },
  "bigIntegerArithmeticProvider": {
    "enabled": true
  },
  "numberResult": {
    "decimalSeparatorLocale": "en-US"
  },
  "integerResult": {
    "enableSeperator": false
  },
  "floatResult": {
    "enableSeperator": false,
    "decimalPlaces": 2
  },
  "percentageResult": {
    "enableSeperator": false,
    "decimalPlaces": 2
  },
  "datetimeResult": {
    "format": "YYYY-MM-DDTHH:mm:ssZ"
  },
  "hexResult": {
    "enablePadding": false,
    "paddingZeros": 8
  },
  "unitOfMeasurementResult": {
    "decimalPlaces": 2,
    "unitNames": false
  }
}